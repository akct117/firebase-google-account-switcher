{
  "name": "firebase-google-account-switcher",
  "version": "1.0.0",
  "description": "A Google Chrome extension allows to switch accounts without frustrations when a selected account does not have permission to see a Firebase project.",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js --watch",
    "build": "yarn clean; webpack --config webpack/webpack.prod.js",
    "clean": "rimraf dist",
    "test": "npx jest",
    "package": "yarn build && zip -r extension.zip dist/;"
  },
  "author": "Jumpei Matsuda",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmatsu/firebase-google-account-switcher"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "moment": "~2.24.0"
  },
  "devDependencies": {
    "@types/chrome": "~0.0.91",
    "@types/jest": "^24.0.19",
    "@types/jquery": "^3.3.30",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "~3.7.2",
    "webpack": "~4.41.2",
    "webpack-cli": "~3.3.10",
    "webpack-merge": "~4.2.1"
  }
}
